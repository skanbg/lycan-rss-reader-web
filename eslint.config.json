{
	"parser": "babel-eslint",
	"rules": {
      "strict": 0,
      "quotes": 0,
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0
	},
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},
	"ecmaFeatures": {
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	},
	"globals": {
		jQuery: true,
		$: true
	}
}